{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-use-before-define": 0,
        "no-underscore-dangle" : 0,
        "radix": 0,
        "no-console" : 0,
        "no-undef":0,
        "no-shadow": 0,
        "no-restricted-globals":0,
        "import/no-extraneous-dependencies": 0
    }
}
